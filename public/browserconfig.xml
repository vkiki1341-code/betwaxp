<?xml version="1.0" encoding="utf-8"?>
<browserconfig>
    <msapplication>
        <tile>
            <square150x150logo src="/android-chrome-192x192.png"/>
            <TileColor>#1e293b</TileColor>
        </tile>
        <notification>
            <polling-uri src="/browserconfig.xml" />
            <polling-uri2 src="/browserconfig.xml" frequency="30" />
            <polling-uri3 src="/browserconfig.xml" frequency="60" />
            <polling-uri4 src="/browserconfig.xml" frequency="1440" />
            <cycle>1</cycle>
        </notification>
    </msapplication>
</browserconfig>
